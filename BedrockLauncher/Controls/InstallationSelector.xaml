<ComboBox x:Class="BedrockLauncher.Controls.InstallationSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BedrockLauncher.Controls"
             xmlns:classes="clr-namespace:BedrockLauncher.Classes"
             xmlns:properties="clr-namespace:BedrockLauncher.Properties"
             xmlns:items="clr-namespace:BedrockLauncher.Controls.Items"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Style="{DynamicResource ComboBoxStyle1}"
             ItemContainerStyle="{DynamicResource ComboBoxItemStyle1}"
             Foreground="White"
             ItemTemplate="{DynamicResource InstallationSelectorItemTemplate}">
</ComboBox>
